\end_inset

 In order to obtain the expected values of 
\begin_inset Formula $\left\langle \overline{H_{\overline{I}}}^{2}\left(t\right)\right\rangle _{\overline{H_{0}}\left(t\right)}$
\end_inset

 respect to the part related to the bath we need to calculate the following
 expected values that appear in the equation 
\begin_inset Formula $\eqref{eq:Hi^2}$
\end_inset

 and can be obtained using the factorization of 
\begin_inset Formula $\eqref{eq:<Hi^2>}$
\end_inset

.
 The expected values relevant for calculations are 
\begin_inset Formula $\left\langle B_{iz}^{2}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 , 
\begin_inset Formula $\left\langle B_{iz}\left(t\right)B_{x}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

, 
\begin_inset Formula $\left\langle B_{iz}\left(t\right)B_{y}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 , 
\begin_inset Formula $\left\langle B_{x}\left(t\right)B_{iz}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 , 
\begin_inset Formula $\left\langle B_{y}\left(t\right)B_{iz}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 , 
\begin_inset Formula $\left\langle B_{x}^{2}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 , 
\begin_inset Formula $\left\langle B_{y}^{2}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

, 
\begin_inset Formula $\left\langle B_{x}\left(t\right)B_{y}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 and 
\begin_inset Formula $\left\langle B_{y}\left(t\right)B_{x}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}$
\end_inset

 .
 Recalling the form of the hamiltonian 
\begin_inset Formula $\overline{H_{\bar{B}}}=\sum_{\mathbf{k}}\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}$
\end_inset

 we can extend the result 
\begin_inset Formula $\eqref{eq:factorization}$
\end_inset

, introducing the notation:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
A_{1}\otimes\cdots\otimes A_{n} & \equiv\bigotimes_{k}A_{k},\\
Z_{\mathbf{k}} & \equiv{\rm Tr}\left({\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)\label{eq:Zk}\\
 & =\left(1-{\rm e}^{-\beta\omega_{\mathbf{k}}}\right)^{-1}\\
 & =f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 with the creation 
\begin_inset Formula $b_{\mathbf{k}}^{\mathbf{}}$
\end_inset

 and annihilation 
\begin_inset Formula $b_{\mathbf{k}}^{\dagger}$
\end_inset

 operators defned in terms of their actions as:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
{\displaystyle b_{\mathbf{k}}^{\mathbf{}}\mid j_{\mathbf{k}}\rangle} & \equiv\sqrt{j_{\mathbf{k}}}\,|j_{\mathbf{k}}-1\rangle,\label{Annihilation}\\
{\displaystyle b_{\mathbf{k}}^{\dagger}\mid j_{\mathbf{k}}\rangle} & \equiv\sqrt{j_{\mathbf{k}}+1}\mid j_{\mathbf{k}}+1\rangle.\label{Creation}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 being 
\begin_inset Formula $|j_{\mathbf{k}}\rangle$
\end_inset

 an eigenstate of 
\begin_inset Formula $H_{\mathbf{k}}\equiv\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}$
\end_inset

.
 With this notation we can write the partition function as:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
Z_{\bar{B}} & ={\rm Tr}\left({\rm e}^{-\beta\sum_{\mathbf{k}}\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right),\\
{\rm e}^{-\beta\sum_{\mathbf{k}}\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}} & =\bigotimes_{\mathbf{k}}{\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}},\label{eq:def Hb}\\
Z_{\bar{B}} & ={\rm Tr}\left(\bigotimes_{\mathbf{k}}{\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)\,\,\text{(by \eqref{eq:def Hb})}\\
 & =\prod_{\mathbf{k}}{\rm Tr}\left({\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)\,\,\text{(by \ensuremath{{\rm Tr}\left(A\otimes B\right)={\rm Tr}\left(A\right){\rm Tr}\left(B\right)})}\\
 & =\prod_{\mathbf{k}}Z_{\mathbf{k}}\,\,\text{(by \eqref{eq:def Hb})}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 For a function 
\begin_inset Formula $f\left(t\right)$
\end_inset

 which can be factorized as:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
f\left(t\right)\equiv\prod_{\mathbf{k}}f_{\mathbf{k}}\left(t\right).\label{eq:def f(t)}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 with 
\begin_inset Formula $f_{\mathbf{k}}\left(t\right)\in{\rm gen}\left(\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}\right)$
\end_inset

, it's expected value is given by:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left\langle f\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}} & =\frac{{\rm Tr}\left(f\left(t\right){\rm e}^{-\beta\overline{H_{\bar{B}}}}\right)}{{\rm Tr}\left({\rm e}^{-\beta\overline{H_{\bar{B}}}}\right)}\\
 & =\frac{{\rm Tr}\left(\prod_{\mathbf{k}}f_{\mathbf{k}}\left(t\right)\bigotimes_{\mathbf{k}}{\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}{{\rm Tr}\left(\bigotimes_{\mathbf{k}}{\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}\,\,\text{(by \eqref{eq:def Hb} and \eqref{eq:def f(t)}) }\\
 & =\frac{{\rm Tr}\left(\bigotimes_{\mathbf{k}}f_{\mathbf{k}}\left(t\right){\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}{{\rm Tr}\left(\bigotimes_{\mathbf{k}}{\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}\\
 & =\frac{\prod_{\mathbf{k}}{\rm Tr}\left(f_{\mathbf{k}}\left(t\right){\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}{\prod_{\mathbf{k}}{\rm Tr}\left({\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}\\
 & =\prod_{\mathbf{k}}\frac{{\rm Tr}\left(f_{\mathbf{k}}\left(t\right){\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}{{\rm Tr}\left({\rm e}^{-\beta\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}\right)}\\
 & =\prod_{\mathbf{k}}\left\langle f_{\mathbf{k}}\left(t\right)\right\rangle _{\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}}.\label{eq:fact fk expect}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 It means that for an operator that can be factorized in terms of functions
 generated by 
\begin_inset Formula $\omega_{\mathbf{k}}b_{\mathbf{k}}^{\dagger}b_{\mathbf{k}}^{\mathbf{}}$
\end_inset

 for each 
\begin_inset Formula $\mathbf{k}$
\end_inset

 we only require to calculate the expected value respect to the Hilbert
 space where the operator belongs.
 This process lead us to the following explicit forms of the expected values
 relevant for our calculations:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left\langle B_{iz}^{2}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}} & =\left\langle \left(\sum_{\mathbf{k}}\left(\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)\right)^{2}\right\rangle _{\overline{H_{\bar{B}}}}\\
 & =\sum_{\mathbf{k}}\!\left\langle \!\left(\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)^{2}\,+\!\sum_{\mathbf{k}\neq\mathbf{k}^{\prime}}\!\left(\!\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)\!\left(\left(g_{i\mathbf{k}^{\prime}}\right.\right.\right.\\
 & \,\,\,\,\,\,\left.\left.\left.-v_{i\mathbf{k}^{\prime}}\!\left(t\right)\right)b_{\mathbf{k}^{\prime}}^{\dagger}+\left(g_{i\mathbf{k}^{\prime}}-v_{i\mathbf{k}^{\prime}}\left(t\right)\right)^{*}b_{\mathbf{k}^{\prime}}^{\mathbf{}}\right)\right\rangle _{\overline{H_{\bar{B}}}}
\end{align}

\end_inset


\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
 & =\sum_{\mathbf{k}}\left\langle \left(\!\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)^{2}\right\rangle _{\overline{H_{\bar{B}}}}\!+\sum_{\mathbf{\mathbf{k}\neq\mathbf{k}^{\prime}}}\!\left\langle \left(\!\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)\!\right\rangle _{\overline{H_{\bar{B}}}}\\
 & \,\,\,\,\,\,\times\left\langle \left(\left(g_{i\mathbf{k}^{\prime}}-v_{i\mathbf{k}^{\prime}}\left(t\right)\right)b_{\mathbf{k}^{\prime}}^{\dagger}+\left(g_{i\mathbf{k}^{\prime}}-v_{i\mathbf{k}^{\prime}}\left(t\right)\right)^{*}b_{\mathbf{k}^{\prime}}^{\mathbf{}}\right)\right\rangle _{\overline{H_{\bar{B}}}}\,\,\text{(by \eqref{eq:fact fk expect}) },\\
\left\langle b_{\mathbf{k}}^{\dagger}\right\rangle _{\overline{H_{\bar{B}}}} & =\frac{{\rm Tr}\left(b_{\mathbf{k}}^{\dagger}\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}|j_{\mathbf{k}}\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\label{eq:expect(bk+)}\\
 & =\frac{{\rm Tr}\left(\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}|j_{\mathbf{k}}\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =\frac{{\rm Tr}\left(\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}\sqrt{\left(j_{\mathbf{k}}+1\right)}|j_{\mathbf{k}}+1\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =\frac{\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}\sqrt{\left(j_{\mathbf{k}}+1\right)}{\rm Tr}\left(|j_{\mathbf{k}}+1\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =\frac{\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}\sqrt{\left(j_{\mathbf{k}}+1\right)}\cdot0}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =0,\\
\left\langle b_{\mathbf{k}}\right\rangle _{\overline{H_{\bar{B}}}} & =\frac{{\rm Tr}\left(b_{\mathbf{k}}\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}|j_{\mathbf{k}}\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\label{eq:expect(bk)}\\
 & =\frac{{\rm Tr}\left(\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}b_{\mathbf{k}}|j_{\mathbf{k}}\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =\frac{{\rm Tr}\left(\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}\sqrt{\left(j_{\mathbf{k}}\right)}|j_{\mathbf{k}}-1\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =\frac{\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}\sqrt{\left(j_{\mathbf{k}}\right)}{\rm Tr}\left(|j_{\mathbf{k}}-1\rangle\negmedspace\langle j_{\mathbf{k}}|\right)}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =\frac{\sum_{j_{\mathbf{k}}}{\rm e}^{-j_{\mathbf{k}}\beta\omega_{\mathbf{k}}}\sqrt{\left(j_{\mathbf{k}}\right)}\cdot0}{f_{{\rm Bose-Einstein}}\left(-\beta\omega_{\mathbf{k}}\right)}\\
 & =0,\\
\left\langle B_{iz}^{2}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}} & =\sum_{\mathbf{k}}\left\langle \!\left(\!\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)^{2}\right\rangle _{\overline{H_{\bar{B}}}}\!+\!\sum_{\mathbf{\mathbf{k}\neq\mathbf{k}^{\prime}}}\!\left(\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)\left\langle b_{\mathbf{k}}^{\dagger}\!\right\rangle _{\overline{H_{\bar{B}}}}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\!\left(t\right)\right)^{*}\left\langle b_{\mathbf{k}}^{\mathbf{}}\!\right\rangle _{\overline{H_{\bar{B}}}}\right)\\
 & \,\,\,\,\,\,\times\left(\left(g_{i\mathbf{k}^{\prime}}-v_{i\mathbf{k}^{\prime}}\left(t\right)\right)\left\langle b_{\mathbf{k}^{\prime}}^{\dagger}\right\rangle _{\overline{H_{\bar{B}}}}+\left(g_{i\mathbf{k}^{\prime}}-v_{i\mathbf{k}^{\prime}}\left(t\right)\right)^{*}\left\langle b_{\mathbf{k}^{\prime}}^{\mathbf{}}\right\rangle _{\overline{H_{\bar{B}}}}\right)\\
 & =\!\sum_{\mathbf{k}}\!\left\langle \!\left(\!\left(g_{i\mathbf{k}}\!-\!v_{i\mathbf{k}}\!\left(t\right)\!\right)b_{\mathbf{k}}^{\dagger}\!+\!\left(g_{i\mathbf{k}}\!-\!v_{i\mathbf{k}}\!\left(t\right)\!\right)^{*}\!b_{\mathbf{k}}^{\mathbf{}}\right)^{2}\right\rangle _{\overline{H_{\bar{B}}}}+\sum_{\mathbf{\mathbf{k}\neq\mathbf{k}^{\prime}}}\!\left(\!\left(g_{i\mathbf{k}}\!-\!v_{i\mathbf{k}}\!\left(t\right)\!\right)\cdot0+\left(g_{i\mathbf{k}}\!-\!v_{i\mathbf{k}}\!\left(t\right)\!\right)^{*}\cdot0\right)\!\left(\!\left(g_{i\mathbf{k}^{\prime}}\!-\!v_{i\mathbf{k}^{\prime}}\!\left(t\right)\!\right)\cdot0\right.\\
 & \,\,\,\,\,\,\left.+\left(g_{i\mathbf{k}^{\prime}}-v_{i\mathbf{k}^{\prime}}\left(t\right)\right)^{*}\cdot0\right)\,\,\text{(by \eqref{eq:expect(bk+)} and \eqref{eq:expect(bk)}) }\\
 & =\left\langle \sum_{\mathbf{k}}\left(\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\left(t\right)\right)b_{\mathbf{k}}^{\dagger}+\left(g_{i\mathbf{k}}-v_{i\mathbf{k}}\left(t\right)\right)^{*}b_{\mathbf{k}}^{\mathbf{}}\right)^{2}\right\rangle _{\overline{H_{\bar{B}}}}\\
