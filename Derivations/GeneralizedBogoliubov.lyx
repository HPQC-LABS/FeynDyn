\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\left\langle \overline{H_{\overline{I}}}^{2}\!\left(t\right)\right\rangle _{\overline{H_{0}}\left(t\right)}\! & =\!\sum_{i}\!\left\langle |i\rangle\negmedspace\langle i|\right\rangle _{\overline{H_{\bar{S}}\!}\left(t\right)}\!\left\langle B_{iz}^{2}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+\!V_{10}^{\Re}\!\left(t\right)\!\sum_{i}\!\left(\left\langle |i\rangle\negmedspace\langle i|\sigma_{x}\right\rangle _{\overline{H_{\bar{S}}}\!\left(t\right)}\!\left\langle B_{iz}\!\left(t\right)\!B_{x}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+\left\langle |i\rangle\negmedspace\langle i|\sigma_{y}\right\rangle _{\overline{H_{\bar{S}}}\!\left(t\right)}\left\langle B_{iz}\!\left(t\right)\!B_{y}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\right)\label{eq:<Hi^2>}\\
 & \,\,\,\,\,\,+\!V_{10}^{\Im}\!\left(t\right)\!\sum_{i}\!\left(\left\langle |i\rangle\negmedspace\langle i|\sigma_{x}\right\rangle _{\overline{H_{\bar{S}}}\left(t\right)}\!\left\langle B_{iz}\!\left(t\right)\!B_{y}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}-\left\langle |i\rangle\negmedspace\langle i|\sigma_{y}\right\rangle _{\overline{H_{\bar{S}}}\left(t\right)}\!\left\langle B_{iz}\!\left(t\right)\!B_{x}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\right)\!+\!V_{10}^{\Re}\!\left(t\right)\!\sum_{i}\!\left(\left\langle \sigma_{x}|i\rangle\negmedspace\langle i|\right\rangle _{\overline{H_{\bar{S}}}\left(t\right)}\right.\\
 & \,\,\,\,\,\,\left.\times\!\left\langle B_{x}\!\left(t\right)\!B_{iz}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+\left\langle \sigma_{y}|i\rangle\negmedspace\langle i|\right\rangle _{\overline{H_{\bar{S}}}\!\left(t\right)}\!\left\langle B_{y}\!\left(t\right)\!B_{iz}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\!\right)\!+\!\left(\!V_{10}^{\Re}\!\left(t\right)\!\right)^{2}\!\!\left(\!\left\langle B_{x}^{2}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+{\rm i}\left\langle \sigma_{z}\right\rangle _{\overline{H_{\bar{S}}}\!\left(t\right)}\!\left\langle B_{x}\!\left(t\right)\!B_{y}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\right.\\
 & \,\,\,\,\,\,\left.-{\rm i}\left\langle \sigma_{z}\right\rangle _{\overline{H_{\bar{S}}}\left(t\right)}\left\langle B_{y}\!\left(t\right)\!B_{x}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+\left\langle B_{y}^{2}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\right)+V_{10}^{\Im}\!\left(t\right)\!\sum_{i}\!\left(\left\langle \sigma_{x}|i\rangle\negmedspace\langle i|\right\rangle _{\overline{H_{\bar{S}}}\!\left(t\right)}\!\left\langle B_{y}\!\left(t\right)\!B_{iz}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}-\left\langle \sigma_{y}|i\rangle\negmedspace\langle i|\right\rangle _{\overline{H_{\bar{S}}}\!\left(t\right)}\right.\\
 & \,\,\,\,\,\,\left.\times\left\langle B_{x}\!\left(t\right)B_{iz}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\right)+\left(V_{10}^{\Im}\!\left(t\right)\right)^{2}\left(\left\langle B_{y}^{2}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+\left\langle B_{x}^{2}\!\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}-{\rm i}\left\langle \sigma_{z}\right\rangle _{\overline{H_{\bar{S}}}\left(t\right)}\left\langle B_{y}\left(t\right)B_{x}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}+{\rm i}\left\langle \sigma_{z}\right\rangle _{\overline{H_{\bar{S}}}\left(t\right)}\right.\\
 & \,\,\,\,\,\,\left.\times\left\langle B_{x}\left(t\right)B_{y}\left(t\right)\right\rangle _{\overline{H_{\bar{B}}}}\right).
\end{align}

\end_inset


\end_layout

